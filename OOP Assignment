class Person:
    def __init__(self, name, age, gender):
        self.name = name
        self.age = age
        self.gender = gender

    def introduce(self):

        print(f"My name is ", self.name, "I'm", self.age, "and I am a ", self.gender)


person = Person('Pascalia', 20, 'Female')
person.introduce()
